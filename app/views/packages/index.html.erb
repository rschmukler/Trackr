<h1><%= link_to "QuÃ¦re", root_path, :id => "logo" %></h1>

<ul id="filters">
  <li><a class="fedex" href="#">Fedex</a></li>
  <li><a class="ups" href="#">UPS</a></li>
  <li><a class="usps" href="#">USPS</a></li>
</ul>

<div id="main">
  <div id="main-controls">
    <a class="add-package" href="#">Add</a>
    <%= link_to "Settings", settings_path, :class => 'settings' %>
  </div>
  <div id="add-package-modal">
    <h1>Add Item</h1>
    <%= form_for @package do |f| -%>
      <p><%= f.text_field :name %> <%= f.label :name, "Name" %></p>
      <p><%= f.text_field :tracking_number %> <%= f.label :tracking_number, "Tracking Number" %></p>
      <p><%= f.select :vendor_id, Vendor.vendors_array %>
      <p><%= f.submit "Add", :disable_with => 'Adding...' %></p>
    <% end -%>
  </div>
  <%= render @packages %>
</div>
